package Bank;

public class Acount {
	private String name;
	private int money;
	
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public float getMoney() {
		return money;
	}
	public void setMoney(int money) {
		this.money = money;
	}
	
	public int plusMoney(int money) {
		this.money += money;
		return this.money;
	}
	public int minusMoney(int money) {
		this.money -= money;
		return this.money;
	}
}
